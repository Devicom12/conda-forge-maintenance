rerender: conda execute -fv conda-forge.github.io/scripts/regenerate_feedstock.py --feedstocks-dir feedstocks --limit 5
pin: conda execute -fv conda-forge.github.io/scripts/pin_the_slow_way.py --feedstocks-dir feedstocks --limit 5
lint: conda execute -fv conda-forge.github.io/scripts/lint_feedstocks.py --feedstocks-dir feedstocks
