rerender: conda execute -fv https://raw.githubusercontent.com/conda-forge/conda-forge.github.io/master/scripts/regenerate_feedstock.py --feedstocks-dir feedstocks --limit 5
pin: conda execute -fv https://raw.githubusercontent.com/conda-forge/conda-forge.github.io/master/scripts/pin_the_slow_way.py --feedstocks-dir feedstocks --limit 5
lint: conda execute -fv conda-forge.github.io/scripts/lint_feedstocks.py --feedstocks-dir feedstocks
